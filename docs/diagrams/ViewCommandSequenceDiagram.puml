@startuml
'https://plantuml.com/sequence-diagram

!include style.puml

box Logic LOGIC_COLOR_T1
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":ViewCommandParser" as ViewCommandParser LOGIC_COLOR
participant ":ViewCommand" as ViewCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as AddressBook MODEL_COLOR
participant "deliveryToView:Person" as Person MODEL_COLOR
end box

AddressBookParser -> ViewCommandParser : parseCommand("view 1")
activate AddressBookParser
activate ViewCommandParser
create ViewCommand
ViewCommandParser -> ViewCommand : ViewCommand(index)
activate ViewCommand
ViewCommand --> ViewCommandParser
deactivate ViewCommand
ViewCommandParser --> AddressBookParser
deactivate ViewCommandParser

AddressBookParser -> ViewCommand : execute()
activate ViewCommand
ViewCommand -> AddressBook : getFilteredPersonList()
activate AddressBook
AddressBook --> ViewCommand : lastShownList
deactivate AddressBook
ViewCommand -> Person : getParcels()
activate Person
Person --> ViewCommand
deactivate Person
ViewCommand -> Person : getName()
activate Person
Person --> ViewCommand
deactivate Person
ViewCommand -> Person : getAddress()
activate Person
Person --> ViewCommand
deactivate Person
ViewCommand -> Person : getEmail()
activate Person
Person --> ViewCommand
deactivate Person
ViewCommand -> Person : getPhone()
activate Person
Person --> ViewCommand
deactivate Person
ViewCommand -> Person : getDeliveryStatus()
activate Person
Person --> ViewCommand
deactivate Person
create CommandResult
ViewCommand -> CommandResult : CommandResult(detailsOfDelivery)
activate CommandResult
CommandResult --> ViewCommand
deactivate CommandResult
ViewCommand --> AddressBookParser
deactivate ViewCommand
deactivate AddressBookParser
@enduml